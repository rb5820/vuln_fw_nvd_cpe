<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        
        <!-- Default CPE-focused NVD Connector -->
        <record id="default_cpe_connector" model="vuln.fw.nvd.connector">
            <field name="name">CPE Dictionary Connector</field>
            <field name="enable_cpe_processing" eval="True"/>
            <field name="auto_create_cpe_dictionary" eval="True"/>
            <field name="cpe_confidence_threshold" eval="0.8"/>
            <field name="batch_size" eval="50"/>
            <field name="active" eval="True"/>
        </record>
        
    </data>
</odoo>